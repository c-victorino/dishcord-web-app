{
  "name": "dish-cord",
  "version": "2.0.0",
  "description": "Web application for creating and managing blog posts",
  "main": "server.js",
  "scripts": {
    "compile:scss": "sass --no-source-map scss:public/css",
    "watch:scss": "sass --no-source-map --watch scss:public/css",
