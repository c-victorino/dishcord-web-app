<section class="mt-7">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h2>{{session.user.userNae}} ( {{session.user.email}} ) History</h2>
        <hr />

        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th class="text-dark">Login Date/Time</th>
                <th class="text-dark">Client Information</th>
              </tr>
            </thead>
            <tbody>
              {{#each session.user.loginHistory}}
                <tr>
                  <td>{{dateTime}}</td>
                  <td>{{userAgent}}</td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</section>